{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2019",
    "lib": [ "ES2019" ],
    "outDir": "out",
    "sourceMap": true,
    "rootDir": "src",
    "strict": true,
    "noUnusedLocals": true,
    "noImplicitOverride": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "noUncheckedIndexedAccess": true
  },
  "exclude": [ "node_modules", ".vscode-test" ]
}
