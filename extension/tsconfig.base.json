{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2019",
    "lib": [ "ES2019" ],
    "composite": true,
    "sourceMap": true,
    "strict": true,
    "noUnusedLocals": true,
    "noImplicitOverride": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "noUncheckedIndexedAccess": true
  }
}
